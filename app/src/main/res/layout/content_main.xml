<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_main" tools:context=".MainActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch Map"
        android:id="@+id/LaunchMap"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="process"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch Market"
        android:id="@+id/LaunchMarket"
        android:layout_below="@+id/LaunchMap"
        android:layout_centerHorizontal="true"
        android:onClick="process"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch Email"
        android:id="@+id/LaunchEmail"
        android:layout_below="@+id/LaunchMarket"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="process"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/SendImage"
        android:layout_below="@+id/LaunchEmail"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="process"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WebView"
        android:id="@+id/btnWebView"
        android:layout_below="@+id/SendImage"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
